{
  "name": "reduxboilergen",
  "version": "1.1.1",
  "description": "Generated Boiler Plate Code for React-Redux. This can be used when you have API calls that needs to have an Request, Response, Error states during different lifecyle of the API call. This is a Command Line tool which will generate the Boiler Plate pattern for your case.",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/chattes/reduxboilergenerator"
  },
  "bin": {
    "boilredux": "build\\index.js"
  },
  "scripts": {
    "test": "jest",
    "build": "babel src -d build",
    "dev": "watch \"npm run build\" src",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run build",
    "release": "np"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "keywords": [
    "react-redux"
  ],
  "author": "Sourav Chatterjee",
  "license": "MIT",
  "dependencies": {
    "fs-path": "0.0.23",
    "promptly": "^2.2.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "jest": "^22.4.2",
    "np": "^2.20.1",
    "prettier": "^1.11.1",
    "watch": "^1.0.1"
  }
}
